{
  "name": "openapi-type-generator",
  "description": "Generate static files for your project from openapi documentation. Schemas, services...",
  "version": "0.1.0",
  "author": "Ferencz David Tamas <ferencz.david@protonmail.com>",
  "bin": {
    "oex": "./bin/run"
  },
  "bugs": "https://github.com/diffy0712/openapi-type-generator/issues",
  "dependencies": {
    "@types/react-dom": "^18.0.6",
    "ink": "^3.2.0",
    "mobx": "^6.6.1",
    "mobx-react-lite": "^3.4.0",
    "node": "^18.5.0",
    "openapi-types": "^12.0.0",
    "react": "^18.0.2",
    "react-dom": "^18.2.0",
    "ts-morph": "^15.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.45",
    "@types/react": "^18.0.17",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/dist"
  ],
  "homepage": "https://github.com/diffy0712/openapi-type-generator",
  "keywords": [
    "openapi",
    "static code generator",
    "typescript",
    "openapi generator"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "oex",
    "dirname": "oex",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "scripts": {
    "generator": "ts-node ./src/run.tsx",
    "lint": "eslint . --ext .ts --config .eslintrc.js"
  },
  "types": "dist/index.d.ts"
}
